<%include _header%>
<h1>Productos Disponibles</h1>

<% armas.forEach((arma)=>{%>
    <div class="panel panel-default">
        <div class="panel-heading">
            <a href="/arma/<%= arma.descripcion %>"><%= arma.descripcion%></a>
        </div>
        <% if(arma.categoria){%>
        <div class="panel-body">
           Categoria:, <%= arma.categoria%>
            <br>
          Precio: ,<%= arma.fuerza%>
            <br>
           Stock:, <%= arma.municiones%>
            <br>
        </div>
        <%}%>
    </div>
    <%})%>
<% include _footer%>